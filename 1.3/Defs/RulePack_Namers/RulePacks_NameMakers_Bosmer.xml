<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RulePackDef>
		<defName>ESCP_NamerPersonBosmerMale</defName>
		<rulePack>
			<rulesStrings>
				<li>name->[MaleList] [FamilyList]</li>
				<li>name(p=0.05)->[MaleList] 'the [a] [Animal]' [FamilyList]</li>
				<li>name(p=0.05)->[MaleList] 'the [a] warden' [FamilyList]</li>
				<li>name(p=0.05)->[MaleList] 'the [b] warden' [FamilyList]</li>
				<li>name(p=0.05)->[MaleList] 'the silent [Weapon]' [FamilyList]</li>
				<li>name(p=0.01)->[MaleList] 'chosen of [BosmerGods]' [FamilyList]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>MaleList</keyword>
					<path>ESCP_BosmerNames/BosmerMale</path>
				</li>
				<li Class="Rule_File">
					<keyword>FamilyList</keyword>
					<path>ESCP_BosmerNames/BosmerFamily</path>
				</li>
			</rulesRaw>
			<rulesFiles>
				<li>BosmerCities->ESCP_BosmerNames/BosmerCities</li>
				<li>a->ESCP_BosmerNames/BosmerFactionA</li>
				<li>b->ESCP_BosmerNames/BosmerFactionB</li>
				<li>c->ESCP_BosmerNames/BosmerFactionC</li>
				<li>BosmerGods->ESCP_BosmerNames/BosmerGods</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>

	<RulePackDef>
		<defName>ESCP_NamerPersonBosmerFemale</defName>
		<rulePack>
			<rulesStrings>
				<li>name->[FemaleList] [FamilyList]</li>
				<li>name(p=0.05)->[FemaleList] 'the [a] [Animal]' [FamilyList]</li>
				<li>name(p=0.05)->[FemaleList] 'the [a] warden' [FamilyList]</li>
				<li>name(p=0.05)->[FemaleList] 'the [b] warden' [FamilyList]</li>
				<li>name(p=0.01)->[FemaleList] 'chosen of [BosmerGods]' [FamilyList]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>FemaleList</keyword>
					<path>ESCP_BosmerNames/BosmerFemale</path>
				</li>
				<li Class="Rule_File">
					<keyword>FamilyList</keyword>
					<path>ESCP_BosmerNames/BosmerFamily</path>
				</li>
			</rulesRaw>
			<rulesFiles>
				<li>BosmerCities->ESCP_BosmerNames/BosmerCities</li>
				<li>a->ESCP_BosmerNames/BosmerFactionA</li>
				<li>b->ESCP_BosmerNames/BosmerFactionB</li>
				<li>c->ESCP_BosmerNames/BosmerFactionC</li>
				<li>BosmerGods->ESCP_BosmerNames/BosmerGods</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
	
</Defs>